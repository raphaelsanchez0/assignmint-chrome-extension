{
  "manifest_version": 3,
  "name": "Assignmint Importer",
  "version": "1.0",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_icon": {
      "16": "logo.png",
      "24": "logo.png",
      "32": "logo.png"
    },
    "default_title": "Assignment Importer",
    "default_popup": "src/pages/popup/index.html"
  },
  "permissions": ["storage", "tabs"],
  "options_page": "src/pages/options/index.html"
}
